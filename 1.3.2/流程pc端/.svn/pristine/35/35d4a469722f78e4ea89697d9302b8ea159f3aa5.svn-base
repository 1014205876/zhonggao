.table_row {
  padding: 0 20px 20px;
}
.txtOverflow{
  width: 300px;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.g_tit h5 {
  float: left;
}

.g_tit a.fr {
  display: block;
  height: 32px;
  line-height: 32px;
  position: relative;
  margin-top: 14px;
}

.g_tit a>i {
  width: 24px;
  height: 24px;
  background-color: #e64340;
  font-size: 12px;
  color: #fff;
  line-height: 24px;
  text-align: center;
  position: absolute;
  top: -12px;
  right: -12px;
  border-radius: 50%;
}

.cur_table {
  position: relative;
  height: calc(100% - 60px);
  overflow: auto;
  overflow-x: scroll;
}

.mask {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3)
}

.confirm {
  position: fixed;
  padding: 20px 0;
  z-index: 9999;
  width: 400px;
  background-color: #fff;
  border: 1px solid #999;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 4px;
}

.confirm .head {
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 20px;
  color: #e64340;
}

.confirm .body {
  line-height: 32px;
  padding: 20px;
  text-align: center;
  font-size: 14px;
  color: #333;
}

.confirm .footer {
  text-align: center;
  padding: 5px 10px 5px 0;
}

.confirm .footer button {
  display: inline-block;
  height: 38px;
  line-height: 38px;
  padding: 0 20px;
  font-size: 14px;
  border-radius: 3px;
  margin: 0 12px;
}

.btn.sure {
  background-color: #589af8;
  color: #fff;
}

.btn.cal {
  background-color: #f26666;
  color: #fff;
}















@media screen and (min-width: 700px) {
  .mainContainer {
    margin: 0px 40px 40px 40px;
  }
}

@media (max-width: 700px) {
  .mainContainer {
    margin: 0px 0px 40px 2px;
  }
}

.mainContainer {
  font-family: sans-serif;
}

.mainContainer a {
  font-weight: 800;
  text-decoration: none;
}

.headlineContainer {
  margin: 80px 0px 40px 5px;
}
